{
    "name": "danihidayatx/image-optimizer",
    "description": "Optimize your Filament images before they reach your database. Forked from joshembling/image-optimizer for Filament v4 support.",
    "keywords": [
        "danihidayatx",
        "laravel",
        "filament",
        "filamentphp",
        "image",
        "optimization",
        "image-optimizer"
    ],
    "homepage": "https://github.com/danihidayatx/image-optimizer",
    "support": {
        "issues": "https://github.com/danihidayatx/image-optimizer/issues",
        "source": "https://github.com/danihidayatx/image-optimizer"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dani Hidayat",
            "role": "Maintainer"
        },
        {
            "name": "Josh Embling",
            "email": "joshembling@gmail.com",
            "role": "Original Creator"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/forms": "^3.0|^4.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "intervention/image": "^2.7",
        "spatie/laravel-package-tools": "^1.19.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DaniHidayatX\\ImageOptimizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DaniHidayatX\\ImageOptimizer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DaniHidayatX\\ImageOptimizer\\ImageOptimizerServiceProvider"
            ],
            "aliases": {
                "ImageOptimizer": "DaniHidayatX\\ImageOptimizer\\Facades\\ImageOptimizer"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
